{
    "name": "p-void",
    "version": "2.0.0",
    "description": "Like Promise.all but voids the return value.",
    "repository": "https://github.com/Richienb/p-void.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "main": "index.js",
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "engines": {
        "node": ">=4"
    },
    "scripts": {
        "lint": "xo",
        "test": "yarn lint && ava"
    },
    "keywords": [
        "promise",
        "void",
        "undefined",
        "transform",
        "wrap",
        "function"
    ],
    "dependencies": {},
    "devDependencies": {
        "ava": "^3.5.1",
        "eslint-config-richienb": "^0.4.2",
        "xo": "^0.29.0"
    },
    "resolutions": {
        "eslint": "^6.8.0"
    },
    "xo": {
        "extends": "richienb",
        "overrides": [
            {
                "files": "test.js",
                "rules": {
                    "node/no-unsupported-features/es-syntax": 0
                }
            }
        ]
    }
}
